{
  "name": "dt-react-component",
  "version": "2.0.0",
  "description": "react-component",
  "repository": "http://gitlab.prod.dtstack.cn/dt-insight-front/infrastructure/dt-react-component",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build-css": "export NODE_ENV='prod' && gulp",
    "increase-memory-limit": "cross-env LIMIT=8096 increase-memory-limit",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "build-storybook": "export NODE_ENV='production' && build-storybook -c .storybook -o dist",
    "compile": "npm run build-css && npm run build-ts",
    "build-ts": "tsc -p tsconfig.build.json",
    "release": "./scripts/release.sh",
    "deploy-storybook": "storybook-to-ghpages",
    "deploy": "./scripts/deploy.sh",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "check-types": "tsc --skipLibCheck",
    "test": "jest --no-chche",
    "test:update": "npm run test -u",
    "lint": "npx eslint './src/**/*.tsx' ",
    "lint-doc": "node ./node_modules/lint-md/bin ./*.md src/stories/markdown/*.md",
    "lint-fix": "npx eslint './src/**/*.tsx' --fix && lint-md ./*.md src/stories/markdown/*.md --fix"
  },
  "keywords": [
    "react",
    "storybook",
    "component"
  ],
  "author": "qingyi",
  "license": "ISC",
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -c ./commitlint.config.js -E HUSKY_GIT_PARAMS"
    }
  },
